<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.airdaydreamers.samplebackswipelibrary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/bsl_App_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/bsl_Theme.Back.Swipe.ActionBar">
        <activity
            android:name="io.github.airdaydreamers.samplebackswipelibrary.MainActivity"
            android:theme="@style/bsl_Theme.Back.Swipe">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="io.github.airdaydreamers.samplebackswipelibrary.activity.FirstActivity"
            android:theme="@style/bsl_Theme.Back.Swipe.ActionBar" />
        <activity android:name="io.github.airdaydreamers.samplebackswipelibrary.activity.SecondActivity" />
        <activity
            android:name="io.github.airdaydreamers.samplebackswipelibrary.activity.FragmentActivity"
            android:theme="@style/bsl_Theme.Back.Swipe.ActionBar" />
    </application>

</manifest>